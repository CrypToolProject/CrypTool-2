<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE category SYSTEM "..\wizard.dtd"[]>
<!-- This is the XML file defining the modern symmetric encryption categories a user can select from. -->
<category image="Sym">
  <name lang="en">Symmetric Encryption</name>
  <name lang="de">Symmetrische Verschlüsselung</name>
  <description lang="en">Selecting this allows you to analyze a ciphertext that was encrypted by a modern symmetric algorithm. You can choose which algorithm you want to use for doing this.</description>
  <description lang="de-DE">Wählen Sie diesen Punkt, um einen Geheimtext, der mit einem modernen symmetrischen Algorithmus verschlüsselt wurde, zu analysieren. Das zu verwendende Verschlüsselungsverfahren kann von Ihnen ausgewählt werden.</description>
  <!-- This is the step's headline. It should be short, but precise. -->
  <headline lang="en">Algorithm Selection</headline>
  <!-- This is the step's description. -->
  <task lang="en">Select a modern symmetric encryption algorithm.</task>
  <!-- AES input -->
  <category image="AES">
    <name lang="en">AES</name>
    <description lang="en">AES is a modern symmetric algorithm to encrypt plaintext or to decrypt ciphertext.
By selecting this, you can analyze some ciphertext that was encrypted with the AES algorithm.
    </description>
    <description lang="de">AES ist ein modernes symmetrisches Verschlüsselungsverfahren.
Wählen Sie diesen Punkt, um einen Geheimtext, der mit dem AES-Algorithmus verschlüsselt wurde, zu analysieren.
    </description>
    <input image="AES">
      <name lang="en">Ciphertext-only</name>
      <name lang="de">Ciphertext-only</name>
      <description lang="en">Analyzes a ciphertext by using only the ciphertext. It will use entropy as the cost function.</description>
      <description lang="de">Analysiert einen Geheimtext (ohne Kenntnis von Klartext(teilen) und ohne Kenntnis des Schlüssels). Verwendet die Entropie als Kostenfunktion.</description>
      <headline lang="en">AES - Ciphertext-Only Analysis</headline>
      <headline lang="de">AES - Ciphertext-only-Analyse</headline>
      <task lang="en">Please input the ciphertext.</task>
      <task lang="de">Bitte geben Sie den Geheimtext ein.</task>
      <inputBox plugin="$Ciphertext$" property="Text" visibleLines="5" alignment="Stretch" width="90%">
        <description lang="en">Ciphertext to analyze (in Hex):</description>
        <description lang="de">Geheimtext, der analysiert werden soll (in Hex):</description>
        <defaultvalue></defaultvalue>
      </inputBox>
      <keyTextBox plugin="$KeySearcher$" property="Key" defaultkey="A1-B1-C1-11-11-11-11-11-11-11-11-11-11-**-**-**" format="[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]">
        <description lang="en">Keypattern:</description>
        <description lang="de">Schlüsselmuster:</description>
      </keyTextBox>
      <comboBox plugin="AES" property="Mode" defaultValue="0" group="properties">
        <description lang="en">Chaining mode:</description>
        <description lang="de">Verkettungsmodus:</description>
        <item content="Electronic Code Book (ECB)" lang="en" />
        <item content="Cipher Block Chaining (CBC)" lang="en" />
        <item content="Cipher Feedback (CFB)" lang="en" />
      </comboBox>
      <comboBox plugin="AES" property="Keysize" defaultValue="0" group="properties">
        <description lang="en">Keysize:</description>
        <description lang="de">Schlüsselgröße:</description>
        <item content="128 Bits" />
        <item content="192 Bits" />
        <item content="256 Bits" />
      </comboBox>
      <sampleViewer image="AES" file="Cryptanalysis\Modern\AES_Analyzer_Entropy.cwm">
        <name lang="en">Ciphertext-only Analysis</name>
        <name lang="de">Ciphertext-only-Analyse</name>
        <description lang="en">Ciphertext-only Analysis Output</description>
        <description lang="de">Ciphertext-only-Analyse: Ausgabe</description>
        <headline lang="en">AES Ciphertext-Only Analysis Output</headline>
        <headline lang="de">Ciphertext-only-Analyse: Ausgabe</headline>
        <task lang="en">Here, you can see the AES analysis.</task>
        <task lang="de">Hier können Sie die AES-Analyse sehen.</task>
        <presentation plugin="$KeySearcher$" width="50%" height="50%">
          <description lang="en">Analyzer:</description>
          <description lang="de">Analysierer:</description>
        </presentation>
        <progressBar plugin="$KeySearcher$">
          <description lang="en">Progress:</description>
          <description lang="de">Fortschritt:</description>
        </progressBar>
        <presentation plugin="$Revealed Plaintext$" height="150" width="50%" alignment="Left" group="Output">
          <description lang="en">Deciphered Ciphertext:</description>
          <description lang="de">Entschlüsselter Geheimtext:</description>
        </presentation>
        <presentation plugin="$Key$" width="50%" alignment="Right" group="Output">
          <description lang="en">Key:</description>
          <description lang="de">Schlüssel:</description>
        </presentation>
      </sampleViewer>
    </input>
    <input image="AES">
      <name lang="en">Known plaintext</name>
      <name lang="de">Bekannter Klartext</name>
      <description lang="en">Analyzes a ciphertext by using a piece of known plaintext.</description>
      <description lang="de">Analysiert einen Geheimtext mit Hilfe eines Stücks bekannten Klartextes.</description>
      <headline lang="en">AES - Known-Plaintext Analysis</headline>
      <headline lang="de">AES - Known-Plaintext-Analyse</headline>
      <task lang="en">Please input the ciphertext and known plaintext.</task>
      <task lang="de">Bitte geben Sie den Geheimtext und den bekannten Klartext ein.</task>
      <inputBox plugin="$Ciphertext$" property="Text" group="inputs" visibleLines="5" alignment="Left" width="50%">
        <description lang="en">Ciphertext to analyze (in Hex):</description>
        <description lang="de">Geheimtext, der analysiert werden soll (in Hex):</description>
        <defaultvalue>73FCE13EDCC504140F6130AB2AB92EB25D2A6117638FDAC8C520B88306986EBC</defaultvalue>
      </inputBox>
      <inputBox plugin="CostFunction" property="RegEx" group="inputs" width="50%" alignment="Right">
        <description lang="en">Known plaintext (as regular expression):</description>
        <description lang="de">Bekannter Klartext (als regulärer Ausdruck):</description>
        <defaultvalue>.*world.*</defaultvalue>
      </inputBox>
      <keyTextBox plugin="$KeySearcher$" property="Key" defaultkey="A1-B1-C1-11-11-11-11-11-11-11-11-11-11-**-**-**" format="[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]-[A-F0-9][A-F0-9]">
        <description lang="en">Keypattern:</description>
        <description lang="de">Schlüsselmuster:</description>
      </keyTextBox>
      <comboBox plugin="AES" property="Mode" defaultValue="0" group="properties">
        <description lang="en">Chaining mode:</description>
        <description lang="de">Verkettungsmodus:</description>
        <item content="Electronic Code Book (ECB)" lang="en" />
        <item content="Cipher Block Chaining (CBC)" lang="en" />
        <item content="Cipher Feedback (CFB)" lang="en" />
      </comboBox>
      <comboBox plugin="AES" property="Keysize" defaultValue="0" group="properties">
        <description lang="en">Keysize:</description>
        <description lang="de">Schlüsselgröße:</description>
        <item content="128 Bits" />
        <item content="192 Bits" />
        <item content="256 Bits" />
      </comboBox>
      <sampleViewer image="AES" file="Cryptanalysis\Modern\AES_Analyzer_KnownPlaintext.cwm">
        <name lang="en">Known-Plaintext Analysis</name>
        <name lang="de">Known-Plaintext-Analyse</name>
        <description lang="en">Known-Plaintext Analysis Output</description>
        <description lang="de">Known-Plaintext-Analyse: Ausgabe</description>
        <headline lang="en">AES Known-Plaintext Analysis Output</headline>
        <headline lang="de">Known-Plaintext-Analyse: Ausgabe</headline>
        <task lang="en">Here, you can see the AES analysis.</task>
        <task lang="de">Hier können Sie die AES-Analyse sehen.</task>
        <presentation plugin="$KeySearcher$" width="50%" height="50%">
          <description lang="en">Analyzer:</description>
          <description lang="de">Analysierer:</description>
        </presentation>
        <progressBar plugin="$KeySearcher$">
          <description lang="en">Progress:</description>
          <description lang="de">Fortschritt:</description>
        </progressBar>
        <presentation plugin="$Revealed Plaintext$" height="150" width="50%" alignment="Left" group="Output">
          <description lang="en">Deciphered Ciphertext:</description>
          <description lang="de">Entschlüsselter Geheimtext:</description>
        </presentation>
        <presentation plugin="$Revealed Key$" width="50%" alignment="Right" group="Output">
          <description lang="en">Key:</description>
          <description lang="de">Schlüssel:</description>
        </presentation>
      </sampleViewer>
    </input>
  </category>
