<UserControl x:Class="AvalancheVisualization.AvalanchePresentation"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AvalancheVisualization"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d" 
             MinHeight="400" MinWidth="650" IsEnabled="True">
    <UserControl.Resources>

        <Style x:Key="buttonStyle"  TargetType="{x:Type Button}">
            <Setter Property="Background" Value="#534b4f"/>
            <Setter Property="FontStyle" Value="Normal"/>
            <Setter Property="Margin" Value="1"/>
            <Setter Property="FontSize" Value="10"/>
            <Setter Property="Foreground" Value="White" />
            <Setter Property="BorderBrush" Value="{x:Null}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border CornerRadius="10" Background="{TemplateBinding Background}"  Padding="5">
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsPressed"  Value="True">
                    <Setter Property="Background" Value="Coral"/>
                </Trigger>
            </Style.Triggers>

        </Style>

        <Style x:Key="textBlockStyle" TargetType="TextBlock">
            <Setter Property="FontStyle" Value="Normal"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="15"/>
        </Style>

        <Style x:Key="RadioButtonStyle" TargetType="RadioButton">
            <Setter Property="FontStyle" Value="Normal"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="FontSize" Value="10"/>
        </Style>

        <Style x:Key="LineButton" TargetType="{x:Type RepeatButton}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type RepeatButton}">
                        <Border Background="#534b4f" BorderBrush="#534b4f" 
            BorderThickness="1" CornerRadius="5">
                            <Path HorizontalAlignment="Center" 
              VerticalAlignment="Center" 
              Data="{Binding Path=Content,
                     RelativeSource={RelativeSource TemplatedParent}}" 
              Fill="white" >
                            </Path>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="PageButton" TargetType="{x:Type RepeatButton}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type RepeatButton}">
                        <Border Background="#66000000" BorderBrush="#66000000"
                         BorderThickness="1" CornerRadius="3" Opacity="0.5">
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="ScrollThumb" TargetType="{x:Type Thumb}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Thumb}">
                        <Grid>
                            <Rectangle RadiusX="3" RadiusY="3" Fill="#534b4f">
                            </Rectangle>
                            <Border CornerRadius="3" BorderBrush="#534b4f">
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="VerticalScrollBar" TargetType="{x:Type ScrollBar}">
            <Setter Property="OverridesDefaultStyle" Value="True"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollBar}">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="18"></RowDefinition>
                                <RowDefinition Height="0.00001*"></RowDefinition>
                                <RowDefinition Height="18"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Border Grid.RowSpan="3" Width="10" CornerRadius="3" BorderBrush="#534b4f" 
                                    BorderThickness="1" Opacity="0.6">
                            </Border>
                            <RepeatButton Grid.Row="0" Style="{StaticResource LineButton}" Height="18" Width="10" 
                                          Command="ScrollBar.LineUpCommand" Content="M 0 4 L 8 4 L 4 0 Z">
                            </RepeatButton>
                            <Track Name="PART_Track" Grid.Row="1" IsDirectionReversed="True">
                                <Track.DecreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource PageButton}" Width="10" Command="ScrollBar.PageUpCommand" />
                                </Track.DecreaseRepeatButton>
                                <Track.Thumb>
                                    <Thumb Style="{StaticResource ScrollThumb}" Width="10" />
                                </Track.Thumb>
                                <Track.IncreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource PageButton}" Width="10" Command="ScrollBar.PageDownCommand" />
                                </Track.IncreaseRepeatButton>
                            </Track>
                            <RepeatButton Grid.Row="2" Style="{StaticResource LineButton}" Height="18" Width="10" 
                                          Command="ScrollBar.LineDownCommand" Content="M 0 0 L 4 4 L 8 0 Z">
                            </RepeatButton>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="HorizontalScrollBar" TargetType="{x:Type ScrollBar}">
            <Setter Property="OverridesDefaultStyle" Value="True"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollBar}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="18"></ColumnDefinition>
                                <ColumnDefinition Width="0.00001*"></ColumnDefinition>
                                <ColumnDefinition Width="18"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Border Grid.ColumnSpan="3" CornerRadius="3" BorderBrush="#534b4f" BorderThickness="1" Opacity="0.6" >
                            </Border>
                            <RepeatButton Grid.Column="0" 
                      Style="{StaticResource LineButton}" 
                      Width="18" Height="13" Command="ScrollBar.LineLeftCommand" 
                      Content="M 4 0 L 4 8 L 0 4 Z">
                            </RepeatButton>
                            <Track Name="PART_Track" Grid.Column="1" IsDirectionReversed="False">
                                <Track.DecreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource PageButton}" 
                              Command="ScrollBar.PageLeftCommand" />
                                </Track.DecreaseRepeatButton>
                                <Track.Thumb>
                                    <Thumb Style="{StaticResource ScrollThumb}" />
                                </Track.Thumb>
                                <Track.IncreaseRepeatButton>
                                    <RepeatButton Style="{StaticResource PageButton}" 
                              Command="ScrollBar.PageRightCommand" />
                                </Track.IncreaseRepeatButton>
                            </Track>
                            <RepeatButton Grid.Column="2" 
                      Style="{StaticResource LineButton}"
                      Width="18" Height="13" Command="ScrollBar.LineRightCommand"
                      Content="M 0 0 L 4 4 L 0 8 Z">
                            </RepeatButton>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>


        <Style x:Key="RightScrollViewer" TargetType="{x:Type ScrollViewer}">
            <Setter Property="OverridesDefaultStyle" Value="True" />

            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ScrollViewer}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition />
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Border BorderThickness="0,1,1,1" Grid.Column="1">
                                <Border.BorderBrush>
                                    <SolidColorBrush Color="{DynamicResource BorderMediumColor}" />
                                </Border.BorderBrush>
                                <ScrollContentPresenter />
                            </Border>
                            <ScrollBar x:Name="PART_VerticalScrollBar"
                     Grid.Column="2"
                     Style="{StaticResource VerticalScrollBar}"
                     Value="{TemplateBinding VerticalOffset}"
                     Maximum="{TemplateBinding ScrollableHeight}"
                     ViewportSize="{TemplateBinding ViewportHeight}"
                     Visibility="{TemplateBinding ComputedVerticalScrollBarVisibility}"/>
                            <ScrollBar x:Name="PART_HorizontalScrollBar"
                     Orientation="Horizontal"
                     Grid.Row="1"
                     Grid.Column="1"
                     Style="{StaticResource HorizontalScrollBar}"
                     Value="{TemplateBinding HorizontalOffset}"
                     Maximum="{TemplateBinding ScrollableWidth}"
                     ViewportSize="{TemplateBinding ViewportWidth}"
                     Visibility="{TemplateBinding ComputedHorizontalScrollBarVisibility}"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </UserControl.Resources>

    <Viewbox>
        <Grid Name="MainGrid" >
            <Grid.Background>
                <LinearGradientBrush EndPoint="0,0" StartPoint="0,1">
                    <GradientStop Color="#1ab3ff" Offset="1"/>
                    <GradientStop Color="#f8f8ff" Offset="0"/>
                </LinearGradientBrush>
            </Grid.Background>
            <Grid.RowDefinitions>
                <RowDefinition Height="210"  />
                <RowDefinition Height="225" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="210"/>
                <ColumnDefinition Width="210"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <!--Home View-->
            <Canvas Name="StartCanvas"  Grid.RowSpan="2" Grid.ColumnSpan="3" Visibility="Visible">
                <TextBlock Name="StartImage" Style="{StaticResource textBlockStyle}" Text="{Loc HomeTitle}" TextAlignment="Center" TextWrapping="Wrap" FontSize="50"  Width="569" Height="383" Canvas.Left="56" Canvas.Top="52">
                    <TextBlock.BitmapEffect>
                        <DropShadowBitmapEffect  Direction="330" Color="Black" ShadowDepth="5" Opacity="0.5" Softness="0.30" />
                    </TextBlock.BitmapEffect>
                </TextBlock>
                <TextBlock Name="initExplanationText" Canvas.Top="200" Canvas.Left="135" Width="400" Foreground="#534b4f" FontSize="16" Text="{Loc Intro}" TextAlignment="Justify" TextWrapping="Wrap" Visibility="Visible">
                </TextBlock>

                <CheckBox x:Name="skip" Canvas.Left="130" Canvas.Bottom="50" Foreground="#534b4f" FontSize="13" Content="{Loc SkipInstructions}">
                </CheckBox>
            </Canvas>

            <!--Buttons-->
            <Button x:Name="doneButton" Style="{StaticResource buttonStyle}" Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2"   Width="62" Height="27" Content="{Loc DoneButton}" FontSize="13" HorizontalAlignment="Center"  VerticalAlignment="Bottom" Margin="0,35,20,35" Visibility="hidden" Click="DoneButton_Click"/>
            <Button x:Name="clearButton" Style="{StaticResource buttonStyle}" Grid.Row="1" Grid.Column="2"  Width="62" Height="27" Content="{Loc ClearButton}" FontSize="13" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="80,35,0,35" Visibility="hidden" Click="ClearButton_Click"/>
            <Button x:Name="toGeneral" Style="{StaticResource buttonStyle}" Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Right"  Width="60" Height="30" Content="{Loc ContinueButton}"  Click="OverviewButton_Click" Margin="5" Visibility="Hidden" />
            <Button x:Name="contButton" Style="{StaticResource buttonStyle}" Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Right"  Width="60" Height="30" Content="{Loc ContinueButton}"  Click="Cont_OnClick" Margin="5" Visibility="hidden" />
            <Button x:Name="backButton" Style="{StaticResource buttonStyle}" VerticalAlignment="Top" HorizontalAlignment="Left"  Width="63" Height="30" Content="{Loc Instructions}"  Click="BackButton_OnClick" Margin="5" Visibility="Hidden" />
            <Button x:Name="ChangesMadeButton" Style="{StaticResource buttonStyle}"  Grid.Column="2" VerticalAlignment="Top" HorizontalAlignment="Right"  Width="64" Height="36" FontSize="9"  Content="{Loc ChangedInput}"  Click="ChangesMadeButton_OnClick" IsEnabled="false"  Margin="5" Visibility="Hidden"/>
            <Button x:Name="inputDataButton" Style="{StaticResource buttonStyle}" VerticalAlignment="Top" HorizontalAlignment="Left"  Width="65" Height="30" FontSize="9" Content="{Loc InputData}" Click="InputDataButton_Click" Margin="5" Visibility="Hidden"/>

            <StackPanel x:Name="buttonsPanel" Orientation="Horizontal" Grid.Row="1" Grid.ColumnSpan="3"  VerticalAlignment="Bottom" Margin="10,20" Visibility="hidden">
                <Button x:Name="afterRound0Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="0"  Click="AfterRound0Button_Click" />
                <Button x:Name="afterRound1Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="1"  Click="AfterRound1Button_Click" />
                <Button x:Name="afterRound2Button"  Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="2"   Click="AfterRound2Button_Click" />
                <Button x:Name="afterRound3Button" Style="{StaticResource buttonStyle}"  Width="30" Height="30" Content="3" Click="AfterRound3Button_Click" />
                <Button x:Name="afterRound4Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="4"   Click="AfterRound4Button_Click" />
                <Button x:Name="afterRound5Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="5"   Click="AfterRound5Button_Click" />
                <Button x:Name="afterRound6Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="6"  Click="AfterRound6Button_Click" />
                <Button x:Name="afterRound7Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="7" Click="AfterRound7Button_Click" />
                <Button x:Name="afterRound8Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="8" Click="AfterRound8Button_Click" />
                <Button x:Name="afterRound9Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="9"  Click="AfterRound9Button_Click" />
                <Button x:Name="afterRound10Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="10"  Click="AfterRound10Button_Click"></Button>
                <Button x:Name="afterRound11Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="11"  Click="AfterRound11Button_Click" Visibility="Collapsed"></Button>
                <Button x:Name="afterRound12Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="12"  Click="AfterRound12Button_Click" Visibility="Collapsed"></Button>
                <Button x:Name="afterRound13Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="13"  Click="AfterRound13Button_Click" Visibility="Collapsed"></Button>
                <Button x:Name="afterRound14Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="14"  Click="AfterRound14Button_Click" Visibility="Collapsed"></Button>
                <Button x:Name="afterRound15Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="15"  Click="AfterRound15Button_Click" Visibility="Collapsed"></Button>
                <Button x:Name="afterRound16Button" Style="{StaticResource buttonStyle}" Width="30" Height="30" Content="16"  Click="AfterRound16Button_Click" Visibility="Collapsed"></Button>
                <Line x:Name="line" Stroke="#008000" StrokeThickness="3" X1="0" Y1="0" X2="10" Y2="30" Visibility="Visible">
                    <Line.BitmapEffect>
                        <DropShadowBitmapEffect Color="Black" Direction="300" ShadowDepth="5" Opacity="0.8" Softness="0.4"  />
                    </Line.BitmapEffect>
                </Line>
                <Button x:Name="instructionsButton" Style="{StaticResource buttonStyle}"  Width="63" Height="30" Content="{Loc Instructions}"  Click="BackButton_OnClick"/>
                <Button x:Name="overviewButton" Style="{StaticResource buttonStyle}" Width="60" Height="30" FontSize="8" Content="{Loc OverviewButton}" Click="OverviewButton_Click"/>
            </StackPanel>

            <!--Instructions-->
            <Grid Name="InstructionsPrep" Grid.ColumnSpan="3" Grid.RowSpan="2" Visibility="Hidden">
                <Grid.RowDefinitions>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="35"/>
                    <RowDefinition Height="95"/>
                    <RowDefinition />
                </Grid.RowDefinitions>
                <TextBlock Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="25" FontWeight="DemiBold" HorizontalAlignment="Center" Text="{Loc InstructionsTitle}" />
                <TextBlock Grid.Row="1" Foreground="White" FontSize="20"   Text="{Loc InstructionsPrep}" Margin="70,0"/>
                <TextBlock Grid.Row="1" Foreground="#534b4f" FontSize="18" FontWeight="ExtraLight" Text="{Loc InstructionsPrepSubtitle}" HorizontalAlignment="Center" Margin="0,3,130,0"/>
                <TextBlock Grid.Row="2" Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsPrepDescription}" TextAlignment="Justify"  TextWrapping="Wrap" Width="550" Margin="0,10,0,0"></TextBlock>

                <StackPanel Grid.Row="3" >
                    <BulletDecorator Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock  Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsPrepBullet1}" TextAlignment="Justify" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>
                    <BulletDecorator Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock  Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsPrepBullet2}" TextAlignment="Justify" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>
                    <BulletDecorator Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsPrepBullet3}" TextAlignment="Justify" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>
                    <BulletDecorator Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsPrepBullet4}" TextAlignment="Justify" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>
                    <BulletDecorator Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsPrepBullet5}" TextAlignment="Justify" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>
                </StackPanel>

                <Button Name="continueButton"  HorizontalAlignment="Right" VerticalAlignment="Top"  Style="{StaticResource buttonStyle}" Width="65" Height="30" FontSize="11" Content="{Loc ContinueButton}" Click="continueButton_Click" Margin="5"/>
            </Grid>

            <Grid Name="InstructionsUnprep" Grid.ColumnSpan="3" Grid.RowSpan="2" Visibility="Hidden">
                <Grid.RowDefinitions>
                    <RowDefinition Height="70"/>
                    <RowDefinition Height="65"/>
                    <RowDefinition />

                </Grid.RowDefinitions>
                <TextBlock Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="25" FontWeight="DemiBold" HorizontalAlignment="Center" Text="{Loc InstructionsTitle}" />
                <TextBlock Grid.Row="1" Foreground="white" FontSize="20"   Text="{Loc InstructionsUnprep}" Margin="70,0"/>
                <TextBlock Grid.Row="1" Foreground="#534b4f" FontSize="17" FontWeight="ExtraLight" Text="{Loc InstructionsUnprepSubtitle}" HorizontalAlignment="Left" Margin="80,25,0,0"/>

                <StackPanel Grid.Row="2">
                    <BulletDecorator Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock  Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsUnprepBullet1}" TextAlignment="Justify" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>
                    <BulletDecorator  Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock  Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsUnprepBullet2}" TextAlignment="Justify" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>
                    <BulletDecorator  Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock  Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsUnprepBullet3}" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>
                    <BulletDecorator Margin="70,5">
                        <BulletDecorator.Bullet>
                            <Ellipse Height="10" Width="10" Fill="#6e7f80"/>
                        </BulletDecorator.Bullet>
                        <TextBlock  Foreground="#534b4f" FontSize="15"  Text="{Loc InstructionsPrepBullet5}" TextAlignment="Justify" Width="550" TextWrapping="Wrap"/>
                    </BulletDecorator>

                </StackPanel>

                <Button Name="continueButtonUnp"  HorizontalAlignment="Right" VerticalAlignment="Top"  Style="{StaticResource buttonStyle}" Width="65" Height="30" FontSize="11" Content="{Loc ContinueButton}" Click="continueButton_Click" Margin="5"/>
            </Grid>

            <!--Titles -->
            <TextBlock Name="buttonsTitle" Grid.Row="1" VerticalAlignment="Bottom" Foreground="#534b4f" FontWeight="DemiBold" Text="{Loc CheckAvalanche}" Visibility="Hidden" Margin="10,55,0,55">
            </TextBlock>
            <TextBlock Name="initStateTitle" Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="25" HorizontalAlignment="Center" Text="{Loc InputTitleAES}" Visibility="Hidden" >
            </TextBlock>
            <TextBlock Name="showChangeTitle" Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="25" HorizontalAlignment="Center" Text="{Loc ShowChangeTitleAES}" Visibility="Hidden" >
            </TextBlock>
            <TextBlock Name="afterRoundsTitle" Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="25" HorizontalAlignment="Center" TextAlignment="Center" Visibility="Hidden" >
            </TextBlock>
            <TextBlock Name="afterRoundsSubtitle" Style="{StaticResource textBlockStyle}"  Grid.ColumnSpan="3" FontSize="20" Foreground="#534b4f" HorizontalAlignment="Center" TextAlignment="Center" Text="{Loc AfterRoundSubtitle}"  Visibility="Hidden" Margin="0,30" >
            </TextBlock>


            <!--Pie Chart -->

            <local:PieChart  x:Name="flippedBitsPiece" Grid.Row="1" Grid.Column="2"  Width="170" Height="170" VerticalAlignment="Top"  HorizontalAlignment="Right" Margin="10,15,-15,15">
                <local:PieChart.ToolTip>
                    <ToolTip x:Name="tp1" Height="40" Background="#6082b6" Foreground="White" FontWeight="Bold">
                    </ToolTip>
                </local:PieChart.ToolTip>
                <local:PieChart.Effect>
                    <DropShadowEffect/>
                </local:PieChart.Effect>

            </local:PieChart>

            <local:PieChart  x:Name="unflippedBitsPiece" Grid.Row="1" Grid.Column="2" Width="170" Height="170" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="10,15,-15,15" >
                <local:PieChart.ToolTip>
                    <ToolTip x:Name="tp2" Height="40" Background="#6082b6" Foreground="White" FontWeight="Bold">
                    </ToolTip>
                </local:PieChart.ToolTip>
                <local:PieChart.Effect>
                    <DropShadowEffect/>
                </local:PieChart.Effect>
            </local:PieChart>

            <Canvas Name="curvedLinesCanvas" Visibility="Hidden">
                <Path Stroke="#534b4f" StrokeThickness="2">
                    <Path.Data>
                        <PathGeometry>
                            <PathGeometry.Figures>
                                <PathFigure StartPoint="185,236">
                                    <!--<BezierSegment Point1="180,180" Point2="185,200" Point3="230,155"></BezierSegment>-->
                                    <QuadraticBezierSegment Point1="190,178" Point2="240,172"/>
                                </PathFigure>
                            </PathGeometry.Figures>
                        </PathGeometry>
                    </Path.Data>
                </Path>

                <Path Stroke="#534b4f" StrokeThickness="2">
                    <Path.Data>
                        <PathGeometry>
                            <PathGeometry.Figures>
                                <PathFigure StartPoint="188,130">
                                    <QuadraticBezierSegment Point1="210,140" Point2="240,140"></QuadraticBezierSegment>
                                    <!--<QuadraticBezierSegment Point1="195,150" Point2="230,120"/>-->
                                </PathFigure>
                            </PathGeometry.Figures>
                        </PathGeometry>
                    </Path.Data>
                </Path>

                <Polygon Points="240,130 250,140, 240,150" StrokeThickness="2"  Stroke="#534b4f" Fill="#007fff" Visibility="Visible" >
                </Polygon>
                <Polygon Points="240,160 250,170, 240,180" StrokeThickness="2"  Stroke="#534b4f" Fill="#007fff" Visibility="Visible" >
                </Polygon>
            </Canvas>

            <Polygon Name="arrow1" Points="175,125 190,140, 175,155" Width="25" Height="35" StrokeThickness="3" Stroke="#4cbb17" Stretch="Fill" HorizontalAlignment="Right"  Visibility="Hidden"  Margin="20,70,15,0">
            </Polygon>
            <Polygon Name="arrow2" Grid.Row="1" Points="175,125 190,140, 175,155" Width="25" Height="35" StrokeThickness="3" Stroke="#4cbb17" Stretch="Fill" HorizontalAlignment="Right" VerticalAlignment="Top" Visibility="Hidden"  Margin="20,50,15,0">
            </Polygon>
            <Polygon Name="arrow3" Grid.Row="1" Grid.ColumnSpan="2" Points="175,125 190,140, 175,155" Width="25" Height="35" StrokeThickness="3" Stroke="#4cbb17" Stretch="Fill" HorizontalAlignment="Center" VerticalAlignment="Top" Visibility="Hidden"  Margin="0,50,50,0">
            </Polygon>
            <CheckBox Name="aesCheckBox" Grid.Row="1"  Foreground="#534b4f" Content="{Loc SingleBitCheckBox}"  Width="170" VerticalAlignment="Bottom" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked" Margin="20,50" Visibility="Hidden"/>

            <!--RadioButtons-->
            <StackPanel Name="radioButtons" Grid.Column="2" Grid.RowSpan="2"  VerticalAlignment="Center" HorizontalAlignment="Center"  Visibility="Hidden">
                <TextBlock Foreground="#534b4f" FontSize="14" Padding="5" Text="{Loc DisplayInformation}">
                </TextBlock>
                <StackPanel Name="radioButtonPanel" Orientation="Horizontal" Margin="10,0">
                    <RadioButton Name="radioDecimal" Foreground="#534b4f" FontSize="10"  Padding="0" Margin="10,0" Checked="RadioButton2Checked" Content="{Loc Decimal}">
                    </RadioButton>
