    xmlns:comps="clr-namespace:Primes.WpfControls.Components">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.5*" />
            <ColumnDefinition Width="0.5*" />
        </Grid.ColumnDefinitions>
        <this:Numbergrid x:Name="numbergrid" Grid.Column="0" MinWidth="400" Margin="10" />
