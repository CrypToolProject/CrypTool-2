<UserControl x:Class="DCAPathVisualiser.UI.PathVisualiserPres"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DCAPathVisualiser.UI"
             mc:Ignorable="d" 
             d:DesignHeight="900" d:DesignWidth="600">
    <Grid>
        <TabControl>
            <TabItem Header="{Loc TabItemHeaderTable}">
                <Viewbox Grid.Row="0" Name="TabItemTable" Stretch="Uniform">

                </Viewbox>
            </TabItem>
            <TabItem Name="TabItemCipher" Header="{Loc TabItemHeaderCipher}" >

            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
