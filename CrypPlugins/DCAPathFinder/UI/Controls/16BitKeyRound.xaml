<UserControl x:Class="DCAPathFinder.UI.Controls._16BitKeyRound"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:DCAPathFinder.UI.Controls"
             mc:Ignorable="d"
             d:DesignHeight="200" d:DesignWidth="300">
    <Canvas Margin="0,0,0,0">
        <!-- InputBits -->
        <!-- Bits zero to three -->
        <Line X1="290.5" Y1="75" X2="290.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0"
              Canvas.Left="1" />
        <Line X1="275.5" Y1="75" X2="275.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="260.5" Y1="75" X2="260.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="245.5" Y1="75" X2="245.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Bits zero to three -->
        <!-- Bits four to seven -->
        <Line X1="210.5" Y1="75" X2="210.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="195.5" Y1="75" X2="195.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="180.5" Y1="75" X2="180.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="165.5" Y1="75" X2="165.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Bits four to seven -->
        <!-- Bits eight to eleven -->
        <Line X1="133.5" Y1="75" X2="133.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="118.5" Y1="75" X2="118.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="103.5" Y1="75" X2="103.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="88.5" Y1="75" X2="88.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Bits eight to eleven -->
        <!-- Bits twelve to fifteen -->
        <Line X1="53.5" Y1="75" X2="53.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="38.5" Y1="75" X2="38.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="23.5" Y1="75" X2="23.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="8.5" Y1="75" X2="8.5" Y2="30" Stroke="{Binding InputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Bits twelve to fifteen -->
        <!-- /InputBits -->
        <!-- Box -->
        <Rectangle Width="300" Height="50" Margin="0,75,0,0" Stroke="{Binding OutputColor, Mode=OneWay}"
                   StrokeThickness="2.0" />
        <!-- /Box -->
        <!-- Xor -->
        <Ellipse Width="30" Height="30" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="1.5" Fill="White"
                 Canvas.Left="135" Canvas.Top="85" />
        <Line X1="135" Y1="100" X2="165" Y2="100" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="150" Y1="86" X2="150" Y2="115" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Xor -->
        <!-- OutputBits -->
        <!-- Bits zero to three -->
        <Line X1="290.5" Y1="170" X2="290.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0"
              Canvas.Left="1" />
        <Line X1="275.5" Y1="170" X2="275.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="260.5" Y1="170" X2="260.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="245.5" Y1="170" X2="245.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Bits zero to three -->
        <!-- Bits four to seven -->
        <Line X1="210.5" Y1="170" X2="210.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="195.5" Y1="170" X2="195.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="180.5" Y1="170" X2="180.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="165.5" Y1="170" X2="165.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Bits four to seven -->
        <!-- Bits eight to eleven -->
        <Line X1="133.5" Y1="170" X2="133.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="118.5" Y1="170" X2="118.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="103.5" Y1="170" X2="103.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="88.5" Y1="170" X2="88.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Bits eight to eleven -->
        <!-- Bits twelve to fifteen -->
        <Line X1="53.5" Y1="170" X2="53.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="38.5" Y1="170" X2="38.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="23.5" Y1="170" X2="23.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <Line X1="8.5" Y1="170" X2="8.5" Y2="125" Stroke="{Binding OutputColor, Mode=OneWay}" StrokeThickness="2.0" />
        <!-- /Bits twelve to fifteen -->
        <!-- /OutputBits -->

    </Canvas>
</UserControl>