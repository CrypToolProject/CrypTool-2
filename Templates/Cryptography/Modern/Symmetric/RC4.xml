  <title lang="en">RC4 Cipher</title>
  <summary lang="en">Usage of the <i>RC4</i> cipher</summary>
  <description lang="en">This template shows the usage of the <i>RC4</i> cipher.</description>
  <keywords>Rivest, Ron, block, symmetric</keywords>
  <replacements lang="en">
    <replacement key="$Sender$" value="Sender" />
    <replacement key="$Receiver$" value="Receiver" />
    <replacement key="$Sender Key$" value="Sender Key" />
    <replacement key="$Receiver Key$" value="Receiver Key" />
    <replacement key="$Sender Message$" value="Sender Message (Plaintext)" />
    <replacement key="$Receiver Message$" value="Receiver Message (decrypted)" />
    <replacement key="$RC4 Encrypt$" value="RC4 Encrypt" />
    <replacement key="$RC4 Decrypt$" value="RC4 Decrypt" />
    <replacement key="$Hex to Bytes$" value="Hex to Bytes" />
    <replacement key="$Keystream$" value="Keystream" />
    <replacement key="$Converter$" value="Converter" />
    <replacement key="$Encryption$" value="Encryption" />
    <replacement key="$Decryption$" value="Decryption" />
    <replacement key="$StringEncoder$" value="StringEncoder" />
    <replacement key="$Transmitted Message (ASCII)$" value="Transmitted Message (ASCII)" />
    <replacement key="$Transmitted Message (Hex)$" value="Transmitted Message (Hex)" />
    <replacement key="$Transmission Channel$" value="Transmission Channel" />
    <replacement key="$text$" value="A secret Message to myself.&#xD;&#xA;This is a test.&#xD;&#xA;Sending messages is kind of fun." />
  </replacements>
