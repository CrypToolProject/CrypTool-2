  <!-- Load assemblies from given subdir -->
  <runtime>
    <!-- Use this switch to make BinaryFormatter fast with large object graphs starting with .NET 4.7.2 -->
    <AppContextSwitchOverrides value="Switch.System.Runtime.Serialization.UseNewMaxArraySize=true"/>
    <loadFromRemoteSources enabled="true"/>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="Lib;LibP2P;CrypPlugins"/>
    </assemblyBinding>
    <enforceFIPSPolicy enabled="false"/>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>
  <applicationSettings>
    <CrypTool.CrypWin.Properties.Settings>
      <setting name="SingletonApplication" serializeAs="String">
        <value>True</value>
      </setting>
    </CrypTool.CrypWin.Properties.Settings>
