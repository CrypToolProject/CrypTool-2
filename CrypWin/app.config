      </setting>
      <setting name="UpdateFlag" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowRibbonBar" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="ShowAlgorithmsNavigation" serializeAs="String">
